<%- include('../template/header')-%> <%- include('../template/navbar')-%>

<div class="container p-4">
  <div class="row">
    <div class="col-md-4">
      <table class="table text-center">
        <thead class="thead-dark">
          <tr>
            <th>#</th>
            <th>Sessiones del dia</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(function(dt,index){%>
          <tr>
            <th scope="row"><%= index+1%></th>
            <td><%= dt.session_id %></td>
          </tr>
          <%}) %>
        </tbody>
      </table>
    </div>
    <div class="col-md-4 ">
      <form action="/seleccionar" method="POST">
        <div class="form-group">
          <button type="submit" class="btn btn-info btn-block">
            Click Sessiones a revisar
          </button>
        </div>
      </form>
    </div>
    <div class="col-md-4" style="display: flex;justify-content: center;align-items: center;">
      <img class="img img-fluid" src="https://media-exp1.licdn.com/dms/image/C4D0BAQELZp03grnjtQ/company-logo_200_200/0/1593228402233?e=2159024400&v=beta&t=7WkbkhHeNoeGy1YeTyAhTqpnKqXhiEzH5EjecW2syiY" >
    </div>
  </div>
</div>
<%- include('../template/footer')-%>